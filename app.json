{
  "expo": {
    "name": "Bundesliga",
    "slug": "Bundesliga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bundesliga-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#D20515",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bundesliga-logo.png",
        "backgroundColor": "#D20515"
      },
      "package": "com.hassoskicker.bundesligachatbot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bundesliga-logo.png",
      "backgroundColor": "#D20515"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bundesliga-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "45104c63-edb3-4198-aeae-ff20e6d1b724"
      }
    }
  }
}
